{
  "ReverseProxy": {
    "Routes": {
      "location-route": {
        "ClusterId": "location.cluster",
        "Match": {
          "Path": "{version:regex(^v[0-9]+$)}/locations/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "/api/{version}/locations/{**catch-all}" } ],
        "AuthorizationPolicy": "Password"
      }
    },
    "Clusters": {
      "location.cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5142"
          }
        }
      }
    }
  },
  "IdentityOption": {
    "Address": "http://localhost:8080/realms/NakliyeTenant",
    "Issuer": "http://localhost:8080/realms/NakliyeTenant",
    "Audience": "gateway.api"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
